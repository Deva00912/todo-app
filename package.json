    "react-dotenv": "^0.1.3",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.14.2",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "react-uuid": "^2.0.0",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3",
    "serve": "^14.2.1",
    "web-vitals": "^2.1.4",
    "workbox-background-sync": "^6.6.0",
    "workbox-broadcast-update": "^6.6.0",
    "workbox-cacheable-response": "^6.6.0",
    "workbox-core": "^6.6.0",
    "workbox-expiration": "^6.6.0",
    "workbox-google-analytics": "^6.6.0",
    "workbox-navigation-preload": "^6.6.0",
    "workbox-precaching": "^6.6.0",
    "workbox-range-requests": "^6.6.0",
    "workbox-routing": "^6.6.0",
    "workbox-strategies": "^6.6.0",
    "workbox-streams": "^6.6.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:local": "set PORT=3003 && cross-env REACT_APP_STAGING=local react-scripts start",
    "start:saga-mongodb": "set PORT=3001 && cross-env REACT_APP_STAGING=saga && cross-env REACT_APP_DATABASE=mongodb  react-scripts start",
    "start:saga": "set PORT=3001 && cross-env REACT_APP_STAGING=saga REACT_APP_DATABASE=firebase react-scripts start",
    "build": " react-scripts build && serve -s build",
    "cypress:open": "cypress open",
    "jsDoc": "jsdoc -c jsDocConfig.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate-docs": "npm run build-storybook -o 'D:/react/todo-app/Documentation/' && npm run jsDoc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.5.1",
    "@storybook/addon-interactions": "7.5.1",
    "@storybook/addon-links": "7.5.1",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.5.1",
    "@storybook/preset-create-react-app": "7.5.1",
    "@storybook/react": "7.5.1",
    "@storybook/react-webpack5": "7.5.1",
    "@storybook/testing-library": "0.2.2",
    "babel-plugin-named-exports-order": "0.0.2",
    "cypress": "^12.17.3",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-storybook": "^0.6.15",
    "prop-types": "15.8.1",
    "react-test-renderer": "^18.2.0",
    "storybook": "7.5.1",
    "webpack": "5.89.0"
  }
}
